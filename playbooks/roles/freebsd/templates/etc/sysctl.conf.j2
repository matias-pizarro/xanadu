# Uncomment this to prevent users from seeing information about processes that
# are being run under another UID.
security.bsd.see_other_uids={{ security_bsd_see_other_uids }}
# see https://www.freebsd.org/doc/handbook/security-intro.html
net.inet.tcp.blackhole={{ net_inet_tcp_blackhole }}
net.inet.udp.blackhole={{ net_inet_udp_blackhole }}
net.inet.icmp.drop_redirect={{ net_inet_icmp_drop_redirect }}
net.inet.ip.redirect={{ net_inet_ip_redirect }}
net.inet.ip.sourceroute={{ net_inet_ip_sourceroute }}
net.inet.ip.accept_sourceroute={{ net_inet_ip_accept_sourceroute }}
net.inet.icmp.bmcastecho={{ net_inet_icmp_bmcastecho }}
